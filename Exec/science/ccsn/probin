&fortin

  ! name of the collapsing star input model
  model_name =  "16wh07.castro.6400"

  ! Initial model mapping parameters

  !- default the minimum model density to just above the Weaklib EOS table lower limit in density
  !- material less dense than min_density will be denoted "fluff"
  min_density = 2.0d3

  !- default the fluff electron fraction to 0.5.
  fluff_ye = 0.5d0

  !- default the minimum model temperature to just above the Weaklib EOS table lower limit in temperature
  min_temperature = 2.0d9

  ! Custom tagging parameters

  !- Always tag if the density exceeds this fraction of the EOS density maximum
  tag_max_density_fraction = 0.1d0

/

&tagging

  max_dengrad_lev    = 6
  max_denerr_lev     = 6

  max_pressgrad_lev  = -1
  max_presserr_lev   = -1

  max_velgrad_lev    = -1
  max_velerr_lev     = -1

  max_temperr_lev    = -1
  max_tempgrad_lev   = -1

  dengrad            = 1.d5
  denerr             = 1.d10

  velgrad            = 1.e18
  velerr             = 1.e18

  pressgrad          = 1.e18
  presserr           = 1.e10

  temperr            = 1.d20
  tempgrad           = 100.e0


/

&sponge

  sponge_upper_density = 1.d4
  sponge_lower_density = 1.d2
  sponge_timescale     = 1.d-3

/

&extern

  eos_input_is_constant = .true.
  weaklib_eos_table_name = "EquationOfStateTable.h5"
  small_x = 1.d-30

/
